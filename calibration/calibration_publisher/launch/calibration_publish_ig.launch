<?xml version="1.0"?>
<launch>
  <arg name="robot_name" default="inspector_gadget"/>

  <node name="calibration_publisher_main" pkg="calibration_publisher" type="calibration_publisher_main">
    <!-- robot_name is used to lookup the extrinsics calibration json file
         so it has to be consistent with the folder names-->
    <param name="robot_name" value="$(arg robot_name)"/>
  </node>

</launch>
