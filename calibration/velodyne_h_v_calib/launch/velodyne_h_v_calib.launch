<launch>
	<param name="/use_sim_time" value="true" type="bool" />
	<group>
		<node pkg="velodyne_h_v_calib" type="velodyne_h_v_calib" name="velodyne_h_v_calib" output="screen" />
		<rosparam command="load" file="$(find ig_calibration_publisher)/calibrations/SensorConfig1/obs/2018_08_14-OLD.yaml" />
		<param name="vvlp_frame_id" value="vvlp_link"/>
		<param name="hvlp_frame_id" value="hvlp_link"/>
		<node pkg="rqt_ez_publisher" type="rqt_ez_publisher" name="rqt_ez_publisher" args="--slider-file $(find velodyne_h_v_calib)/config/rqt_ez_pub_config.yaml" />
		<node pkg="rviz" type="rviz" name="rviz" args="-d $(find velodyne_h_v_calib)/config/velodyne_h_v_calib_config.rviz" />
	</group>
</launch>
